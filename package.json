{
  "name": "@derpenstiltskin/hudu-node",
  "version": "0.1.0",
  "description": "Hudu API Wrapper",
  "main": "./dist/cjs/index.js",
  "module": "./dist/mjs/index.js",
  "types": "./dist/mjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "mocha": "nyc mocha --config=test/.mocharc.js",
    "test": "npm run lint && npm run mocha",
    "clean": "run-script-os",
    "clean:win32": ".\\build\\clean.bat",
    "clean:darwin:linux": "./build/clean.sh",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/derpenstiltskin/hudu-node.git"
  },
  "keywords": [
    "hudu",
    "api"
  ],
  "author": "Dustin Riley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/derpenstiltskin/hudu-node/issues"
  },
  "homepage": "https://github.com/derpenstiltskin/hudu-node",
  "devDependencies": {
    "typescript": "^4.6.3",
    "@types/node": "^17.0.23",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-prettier": "^4.0.0",
    "run-script-os": "^1.1.6",
    "mocha": "^9.2.2",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "nyc": "^15.1.0",
    "nock": "^13.2.4",
    "prettier": "^2.6.2"
  },
  "files": [
    "dist/"
  ]
}
